}
.t5, .t5 .botaoFade {
    background: url("../images/new/botaoT5.png") repeat scroll 0 0 transparent;
    height: 22px;
    width: 90px;
}
.botaoFade {
    background-position: 0 bottom;
    background-repeat: no-repeat;
    display: block;
    margin: 0;
    padding: 0;
    position: relative;
}
.botaoFade .botaoFade {
    background-attachment: inherit;
    background-clip: inherit;
    background-color: inherit;
    background-image: inherit;
    background-origin: inherit;
    background-position: 0 top;
    background-repeat: inherit;
    background-size: inherit;
    height: 100%;
    left: 0;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 1;
}
.botaoFade input {
    background: none repeat scroll 0 0 transparent;
    border: 0 none;
    color: #7C7C7C;
    cursor: pointer;
    display: block;
    font: 1em Verdana,Geneva,sans-serif;
    height: 100%;
    left: 0;
    margin: 0;
    padding: 0;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 2;
}
.ac .botaoFade input {
    color: #000000;
}
