}
ul.menu li.subMenu ul {
	background-color: #E0E0E0;
    border-left: 2px solid #FFFFFF;
    display: none;
    float: left;
    left: 173px;
    list-style: none outside none;
    margin: 0;
    padding: 0;
    position: absolute;
    top: 0;
    width: 174px;
    border-bottom: 1px solid #FFFFFF;
    border-right: 1px solid #FFFFFF;    
}
ul.menu li.subMenu ul.largo {
	width:250px;
}
ul.menu a.largo{
	width:250px;
}
ul.menu a span.largo {
	width:241px;
}
ul.menu a span.largoNot {
	width:246px;
}
